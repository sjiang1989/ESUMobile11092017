<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <permission android:name="edu.school.app.C2D_MESSAGE"
        android:protectionLevel="signature" />
    <uses-permission android:name="edu.school.app.permission.C2D_MESSAGE"/>

    <application
        android:name="com.ellucian.mobile.android.EllucianApplication">
        <!-- GCM receiver -->
        <receiver android:name="com.ellucian.mobile.android.notifications.GcmBroadcastReceiver"
            android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.REGISTRATION"/>
                <category android:name="edu.school.app"/>
            </intent-filter>
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE"/>
                <category android:name="edu.school.app"/>
            </intent-filter>
        </receiver>
    </application>
</manifest>